<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/draw_layout">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.example.user.vit.HomePage"
        android:id="@+id/activity_home_page">
        <include
            android:id="@+id/toolbar"
            layout="@layout/app_bar"/>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/frame"></FrameLayout>

    </LinearLayout>

    <fragment
        android:layout_width="280dp"
        android:layout_height="match_parent"
        android:id="@+id/nav_draw"
        app:layout="@layout/fragment_nav__draw"
        android:layout_gravity="start"
        android:name="com.example.user.vit.fragment.Nav_Draw"
        tools:layout="@layout/fragment_nav__draw"></fragment>

</android.support.v4.widget.DrawerLayout>
